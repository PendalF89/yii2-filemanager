{
    "name": "douglasmk/yii2-filemanager",
    "description": "Yii2 filemanager",
    "type": "yii2-extension",
    "license": "GNU GPLv2",
    "authors": [
        {
            "name": "DouglasMK",
            "contact": "www.douglasmk.com.br"
        }
    ], 
    
	"repositories": [
		{
            "type": "composer",
            "url": "http://localhost:4680"
        }
    ],   

    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-imagine": "*",
        "pendalf89/yii2-tinymce": "*",
        "kartik-v/yii2-widget-alert": "*",
        "2amigos/yii2-file-upload-widget": "*",
        "obsrepo/module-palavraschave": "dev-master",
        "obsrepo/yii2-obsref": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "douglasmk\\filemanager\\": ""
        }
    }
}
